{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "channel": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "preview-sim": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "channel": "preview-sim"
    },
    "production": {
      "channel": "production"
    },
    "development-sim": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "channel": "development-sim"
    }
  },
  "submit": {
    "production": {}
  }
}
